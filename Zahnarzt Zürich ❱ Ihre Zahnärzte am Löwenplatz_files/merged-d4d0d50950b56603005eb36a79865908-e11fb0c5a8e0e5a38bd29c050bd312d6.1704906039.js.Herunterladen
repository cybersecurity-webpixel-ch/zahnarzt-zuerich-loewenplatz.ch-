function obscureAddMid() {
	document.write('&#64;');
}
function obscureAddEnd() {
	document.write('&#46;');
}


function osScrollTo(top) {
	// Add little offset space
	// Navbar Toggle always shown, so substract it from scroll position
	var offset = 15,
		navbarToggleHeight = getFixedHeaderHeight();
		
	jQuery('html,body').animate({scrollTop:top-navbarToggleHeight-offset}, '500', 'swing');
}

function getInnerFullHeight($elements, selector) {
	var h = 0;
	for(var i in $elements)
		if($elements[i].find(selector).length)
			h += Math.floor($elements[i].find(selector).outerHeight());
	return h;
}

function getFixedHeaderHeight() {
	return getInnerFullHeight(getFixedHeaderElements(), '> div');
}

function getFixedHeaderElements() {
	return [
		$('#header > .page-header'), 
		$('#header > .page-menu')
	];
}

function updateFix($pelement, $aBefore) {
	$pelementInner = $pelement.find('> div');
	// Update outer height of parent placeholder element 
	$pelement.height($pelementInner.outerHeight());
	// Set top offset of fixed inner element
	var h = getInnerFullHeight($aBefore, '> div');
	// If not first element, push one pixel below
	//if($aBefore.length > 0) h++;
	
	$pelementInner.css('top',h);
}

/**
 * Affix header element (BETA)
 * TODO: Add options to dynamically add elements / multiple elements / ...
 * @author: Kabaz (Kai Vier)
 **/
function headerAffix() {
	const outerFrame = '#header';
	const affixElements = getFixedHeaderElements();
	
	var $pheader = affixElements[0];
	var $pmenu = affixElements[1];
	
	$pheader.addClass('fixed-header-element');
	$pmenu.addClass('fixed-header-element');
	
	// If layout header, menu, ...
	updateFix($pheader, []);
	updateFix($pmenu, [$pheader]);
	$(window).on('resize', function() {
		updateFix($pheader, []);
		updateFix($pmenu, [$pheader]);
	});
}

/**
 * Offcanvas
 **/
$(function() {
	$('#js-bootstrap-offcanvas').prepend('<i class="fa fa-bars offcanvas-toggle" aria-hidden="true" data-toggle="offcanvas" data-target="#js-bootstrap-offcanvas"></i>');
	$('.offcanvas-toggle').on('click', function(e) {
		e.preventDefault();
		toggleNav();
	});	
	$('#js-bootstrap-offcanvas .nav-link').on('click', function() {
		// If nav-link has href, toggle (close) nav (could be anchor link on same page)
		if(typeof $(this).attr('href') !== 'undefined')
			toggleNav();
	});
});

function toggleNav() {
	$joc = $("#js-bootstrap-offcanvas");
	$jtoggle = $('.offcanvas-toggle');
	if($joc.hasClass('in')) {
		$joc.removeClass('in');
		$jtoggle.removeClass('is-open');
	} else {
		$joc.addClass('in');
		$jtoggle.addClass('is-open');
	}
}


/**
 * Nav Collapser
 **/
$(function() {
	
	$('.nav .collapser').click(function(e) {
		e.preventDefault();
		toggleSubNav($(this).parent());
		return false;
	});
	
	$('.nav .nav-link').click(function(e) {
		if(typeof $(this).attr('href') === 'undefined') {
			e.preventDefault();
			toggleSubNav($(this).parent());
console.log("toggle link");
			return false;
		}
	});
	
});

function toggleSubNav($jNavItem) {
	if($jNavItem.hasClass('open')) {
		$jNavItem.attr('aria-expanded', 'false').removeClass('open');
		$jNavItem.find('> ul').slideUp();
	} else {
		$jNavItem.attr('aria-expanded', 'true').addClass('open');
		$jNavItem.find('> ul').slideDown();
	}
}

/**
 * Initial scroll if marker
 */
jQuery(window).load(function() {
	if(location.href.indexOf('#') > 0) {
		var hash = location.href.substring(location.href.indexOf('#'));
		
		if(jQuery(hash).length)
			window.setTimeout(function() {
				osScrollTo(jQuery(hash).offset().top);
			}, 500);
	}
});

jQuery(document).ready(function() {
	
	// Panel collapse scroll to if hidden
	jQuery(".collapse").on('shown.bs.collapse', function(){
		var ot = jQuery(this).offset().top,
			wt = jQuery(window).scrollTop();
			
			if(ot < wt)
				osScrollTo(jQuery(this).parent().offset().top);
	});
});

function isUtilityLink($element) {
	if($element.hasClass('popup'))
		return true;
	
	if($element.hasClass('carousel-control'))
		return true;
		
	// If is accordion / panel collapse / data-toggle
	if($element.hasClass('btn-accordion') || typeof $element.attr('data-toggle') != 'undefined')
		return true;
	
	// If is slidedown
	if($element.hasClass('slidedown') || $element.closest('.slidedown').length > 0)
		return true;
	
	return false;
}

/**
 * Anchor link slider 
 **/
function anchorLinkListener(e) {
	// If link is utility link / has another functionality
	if(isUtilityLink($(this)))
		return;
	
	// If no href anchor present, do nothing
	if(typeof $(this).attr('href') === 'undefined')
		return;
			
	var href = jQuery(this).attr('href'),
		hash = href.substring(href.indexOf('#'));
		
	// If external Url do nothing
	if(href.indexOf('http') >= 0 && href.indexOf(location.host) < 0)
		return;
		
	// If anchor is empty, do nothing
	if(hash === '#')
		return;
	
	// Slide to element
	var jE = jQuery(hash);
	if(jE.length) {
		e.preventDefault();
		
		var top = jE.offset().top;
		osScrollTo(top);
	}
	
	// Open panel if necessary
	if(jE.hasClass('panel') && !jE.find('.panel-collapse').hasClass('in')) {
		jE.find('.panel-heading .btn').click();	//jE.collapse('show');
	}
}

/* Anchor link slider */
jQuery('body').on('click', 'a[href*="#"]', anchorLinkListener);


/*
 * Up-Btn link
 */
jQuery('body').on('click', 'a.up-btn', function(e){
	e.preventDefault();
	osScrollTo(0);
});


/*
 * tel-Link action for Desktop
 */
jQuery('body').on('click', 'a[href^=tel]', function(e){
	if(jQuery(window).width() >= 768) {
		e.preventDefault();
		var m = jQuery('#call-modal');
		if(m.length)
			m.modal('show');
	}
});

/**
 * Checkbox toggle style
 **/
$(function() {
	jQuery('input[type=checkbox]').on('change', function(e) {
		if(jQuery(this)[0].checked)
			jQuery(this).closest('.checkbox').addClass('checked');
		else
			jQuery(this).closest('.checkbox').removeClass('checked');	
	});
});
// Set default optout messages
var gaOptoutMessage = {
	de: 'Sie haben sich erfolgreich von der Google Analytics Messung ausgeschlossen',
	en: 'You have been excluded from Google Analytics recording successfully'
}

// accept custom optout messages from constants defined before
if(typeof GAOPTOUT_MESSAGE_DE != 'undefined')	gaOptoutMessage.de = GAOPTOUT_MESSAGE_DE;
if(typeof GAOPTOUT_MESSAGE_EN != 'undefined')	gaOptoutMessage.en = GAOPTOUT_MESSAGE_EN;

/**
 * Enable gaOptout links by class-Attribute: gaOptout
 */
window.onload = function() {
	var matches = document.querySelectorAll('.gaOptout');
	matches.forEach(function(item) {
		item.onclick = function() {
			gaOptout();
			if(item.className.indexOf('en')>=0) alert(gaOptoutMessage.en);
			else alert(gaOptoutMessage.de);
		};
	});
};

/**
 * Enable gaOptout links by rel-Attribute: gaOptout
 */
jQuery(window).load(function() {
	jQuery('body').on('click', 'a[rel=gaOptout]', function(e) {
		e.preventDefault();
		gaOptout();
		if($(this).hasClass('en')) alert(gaOptoutMessage.en);
		else alert(gaOptoutMessage.de);
		return false;
	});
});


/* DrFlex Popup Listener */
jQuery('body').on('click', '.toggleDrFlexAppointments', function(e){
	e.preventDefault();
	toggleDrFlexAppointments();
});

var modalTemplate = '<div class="modal fade" id="myModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"><div class="modal-dialog"><div class="modal-content">'
+ '<div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-label="Schlieen"><span aria-hidden="true">&times;</span></button><h3 class="modal-title">Modal Title</h3></div>'
+ '<div class="modal-body"></div>'
+ '<div class="modal-footer"><button type="button" class="btn btn-default" data-dismiss="modal">Schlieen</button></div>'
+ '</div></div></div>';

function initPopupContent(jpc) {
	var jM = jQuery(modalTemplate).clone(true,true);
	
	if(jpc.attr('id') == "")
		return;
	
	jM.attr('id', 'popup-'+jpc.attr('id'));
	
	var jTitle = jpc.find('> h1, > h2, > h3, > p');
	jM.find('.modal-title').html(jTitle.html());
	var jBody = jpc.find('.ce-bodytext').html();
	jM.find('.modal-body').html(jBody);
	
	jQuery('body').append(jM);
}

/**
 * Initialize Auto Popup
 *
 * lib = fancybox | magnific | ...
 */
function initPopupAuto(lib) {
	console.log("Cookie Popup: Init ("+lib+")");	
	
	if(typeof lib == "undefined" || lib == "") {
		console.error("Cookie Popup: Popup library not defined!");
		return;
	}
	
	jQuery('.popup-content.popup-auto').each(function() {
		var id = jQuery(this).attr('id');
		if(typeof id != "undefined" && id != "") {
			// return, if the cookie of this popup is already set (popup should not be displayed)
			var dataCookie = jQuery(this).attr('data-popup-cookie');
			if(typeof dataCookie != "undefined" && dataCookie != "" && ipGetCookie(dataCookie))
				return;

			console.log("Cookie Popup: Open ("+jQuery(this).attr('id')+")");
			
			// Create popup
			if(lib == 'fancybox')
				createPopupFancybox(id, jQuery(this));
			else if(lib == 'magnific')
				createPopupMagnific(id, jQuery(this));
			else if(lib == 'simplemodal')
				createPopupSimpleModal(id, jQuery(this));
			else if(lib == 'bootstrapmodal')
				createPopupBootstrapModal(id, jQuery(this));
			
			// Set cookie
			if(typeof dataCookie != "undefined" && dataCookie != "")
				ipSetCookie(dataCookie,1);
		}
	});
}
function createPopupFancybox(id, jThis) {
	var dataStyle = jThis.attr('data-popup-style');
	var baseClass = (typeof dataStyle != "undefined" && dataStyle != "") ? dataStyle : '';

	jQuery.fancybox.open({
		src  : '#'+id,
		type : 'inline',
		baseClass : baseClass
	});
}
function createPopupMagnific(id, jThis) {
	$.magnificPopup.open({
		items: {
			src: '#'+id 
		},
		type: 'inline'
	}, 0);
}
function createPopupSimpleModal(id, jThis) {
	$("#"+id).modal();	
}
function createPopupBootstrapModal(id, jThis) {
	var jMd = jQuery('#popup-'+id).length ? jQuery('#popup-'+id) : jQuery('#'+id);
	if(jMd.length) {
		jMd.modal('show');
	}
}
function ipSetCookie(name,value,days) {
    var expires = "";
    if (days) {
        var date = new Date();
        date.setTime(date.getTime() + (days*24*60*60*1000));
        expires = "; expires=" + date.toUTCString();
    }
    document.cookie = name + "=" + (value || "")  + expires + "; path=/";
}
function ipGetCookie(name) {
    var nameEQ = name + "=";
    var ca = document.cookie.split(';');
    for(var i=0;i < ca.length;i++) {
        var c = ca[i];
        while (c.charAt(0)==' ') c = c.substring(1,c.length);
        if (c.indexOf(nameEQ) == 0) return c.substring(nameEQ.length,c.length);
    }
    return null;
}
function getFullYear() {return new Date().getFullYear()}
function getMonthFullYear() {
  const months = ["Januar","Februar","Mrz","April","Mai","Juni","Juli","August","September","Oktober","November","Dezember"];
  return months[(new Date()).getMonth()] + " " + new Date().getFullYear();
}
function printAutoDates() {
  var els = document.getElementsByClassName('print-current-year');
  for(var i=0; i<els.length;i++) {els[i].innerHTML = getFullYear()};
  els = document.getElementsByClassName('print-current-month-year');
  for(var i=0; i<els.length;i++) {els[i].innerHTML = getMonthFullYear()};
}
window.addEventListener('load', printAutoDates);

var modalContent = '<div class="modal fade" id="myModel" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true"><div class="modal-dialog"><div class="modal-content">'
+ '<div class="modal-header"><button type="button" class="close" data-dismiss="modal" aria-label="Schlieen"><span aria-hidden="true">&times;</span></button><h3 class="modal-title">Modal Title</h3></div>'
+ '<div class="modal-body"></div>'
+ '<div class="modal-footer"><button type="button" class="btn btn-default" data-dismiss="modal">Schlieen</button></div>'
+ '</div></div></div>';

var jM = null;
	
jQuery(document).ready(function() {
	
	// Modal
	jM = jQuery(modalContent);
	jM.attr('id', 'callPopupModal');
	var jTitle = jQuery('#call-popup > h1, #call-popup > h2, #call-popup > h3, #call-popup > p');
	jM.find('.modal-title').html(jTitle.html());
	var jBody = jQuery('#call-popup .ce-bodytext').html();
	jM.find('.modal-body').html(jBody);
	
	jQuery('body').append(jM);
	
	
	
	// Panel collapse scroll to if hidden
	jQuery(".collapse").on('shown.bs.collapse', function(){
		var ot = jQuery(this).offset().top,
			wt = jQuery(window).scrollTop();
			
			if(ot < wt)
				osScrollTo(jQuery(this).parent().offset().top);
	});
});
	

/*
 * Overwrite target for internal links (t3 rte bugfix)
 */
jQuery('a.internal-link').attr('target', '_top');

/**
 * Init popups
 **/
$(document).ready(function() {
	// Popups from classes
	jQuery('.popup-content').each(function() {
		var jpc = jQuery(this);
		initPopupContent(jpc);		
	});
	// Auto Popup
	initPopupAuto('bootstrapmodal');
	// Set popup link listeners
	jQuery('body').on('click', 'a.popup', function(e) {
		e.preventDefault();
		
		var href = jQuery(this).attr('href'),
			hash = href.substring(href.indexOf('#'));
		
		var jMd = jQuery(hash);
		if(jMd.length)
			jMd.modal('show');
	});
});

/**
 * Fixed Header initialization
 */
$(window).on('load', function () {
	headerAffix();
});

